<#ftl output_format="XML" auto_esc=true>
<#compress>
<response>
    <#-- Where code is a 'SUCCESS' or 'FAILED' String -->
    <returncode>${returnCode}</returncode>
    <recordings>
        <#list recordings as r>
            <recording>
                <#if r.hasError()>
                    <error>
                        <recordID>${r.getMetadataXml()}</recordID>
                    </error>
                <#else>
                    <#include "include-recording.ftlx">
                </#if>
            </recording>
        </#list>
    </recordings>
</response>
</#compress>